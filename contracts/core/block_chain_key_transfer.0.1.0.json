{
"header": {
  "title":"Block Chain Key Transfer",
  "description":"Revealed Transfer of a BTC Signing Key",
  "template_url":"https://github.com/VaultTree/contracts/blob/master/contracts/block_chain_key_transfer.0.1.0.json",
  "contributer":"Andrew Bashelor"
},
"vaults": {

"sender_secret":{
  "owner": "sender",
  "fill_with": "RANDOM_NUMBER",
  "lock_with": "MASTER_PASSPHRASE",
  "unlock_with": "MASTER_PASSPHRASE",
  "contents": ""
},

"concealed_destination_address":{
  "owner": "sender",
  "fill_with": "EXTERNAL_DATA",
  "lock_with": "CONTENTS['sender_secret']",
  "unlock_with": "CONTENTS['sender_secret']",
  "contents": ""
},

"origin_address":{
  "owner": "sender",
  "fill_with": "EXTERNAL_DATA",
  "lock_with": "UNLOCKED",
  "unlock_with": "UNLOCKED",
  "contents": ""
},

"btc_signing_key":{
  "owner": "sender",
  "fill_with": "EXTERNAL_DATA",
  "lock_with": "CONTENTS['concealed_destination_address']",
  "unlock_with": "CONTENTS['revealed_destination_address']",
  "contents": ""
},

"receiver_secret":{
  "owner": "receiver",
  "fill_with": "RANDOM_NUMBER",
  "lock_with": "MASTER_PASSPHRASE",
  "unlock_with": "MASTER_PASSPHRASE",
  "contents": ""
},

"revealed_destination_address":{
  "owner": "receiver",
  "fill_with": "EXTERNAL_DATA",
  "lock_with": "CONTENTS['receiver_secret']",
  "unlock_with": "CONTENTS['receiver_secret']",
  "contents": ""
}

}
}
